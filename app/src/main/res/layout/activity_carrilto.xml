<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/quemasve"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fodoazul"
    android:clickable="false"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.proyecto2pdm115.Carrito">

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:text="Carrito de Compras"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textIsSelectable="false"
        android:textSize="24sp"
        android:textStyle="bold" />

    <ListView
        android:id="@+id/Pedidos"
        android:layout_width="wrap_content"
        android:layout_height="574dp"
        android:layout_above="@+id/aaaa"
        android:layout_below="@+id/imageButton21"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginBottom="35dp" />

    <TextView
        android:id="@+id/tvSelectDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="16dp"
        android:layout_marginTop="600dp"
        android:layout_marginEnd="29dp"
        android:text="Fecha de solicitud de medicamentos"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/etSelectDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="16dp"
        android:layout_marginTop="620dp"
        android:layout_marginEnd="79dp"
        android:hint="Seleccionar fecha"
        android:textColor="@android:color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvSelectDate" />

    <ImageButton
        android:id="@+id/imageButton21"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_alignTop="@+id/textView10"
        android:layout_alignEnd="@+id/Pedidos"
        android:layout_alignRight="@+id/Pedidos"
        android:background="@drawable/check" />

    <ImageButton
        android:id="@+id/Clear"
        android:layout_width="48dp"
        android:layout_height="45dp"
        android:layout_above="@+id/Pedidos"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:background="@android:drawable/btn_dialog" />

    <ImageButton
        android:id="@+id/aaaa"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:background="@drawable/circulo"
        android:backgroundTint="@color/teal_700"

        android:onClick="osdo"
        android:src="@android:drawable/ic_menu_upload" />


</RelativeLayout>
